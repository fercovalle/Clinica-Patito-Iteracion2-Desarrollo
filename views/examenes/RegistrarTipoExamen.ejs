<!doctype html>

<html lang="es">

    <%-include('../headerGeneral');%>

<body>
    <%-include('../menus/MenuAdministracion');%>
    <div class="container-fluid">
        <form>
            <h1>Registrar Tipo de Examen</h1>
            <div class="mb-3">
                <label for="testType" class=f orm-label>Nombre para tipo de examen:</label>
                <input type="number" class="form-control" id="sampleType" placeholder="Ingrese nombre del nuevo tipo de Muestra" required>
            </div>
            <div class="mb-3">
                <label for="sampleTypeUnit" class="form-label">Precio de examen:</label>
                <input type="text" class="form-control" id="sampleTypeUnit" placeholder="Ingrese el precio del examen">
            </div>
            <div class="mb-3">
                <label for="testTypeStatus" class="form-label">Estado del tipo de examen:</label>
                <input class="form-control" list="datalistOptions" id="testTypeStatus" placeholder="Seleccione estado del examen" required>
                <datalist id="datalistOptions">
                    <option value="Habilitado">
                    <option value="Deshabilitado">
                  </datalist>
            </div>
            <button type="submit" class="btn btn-primary">Registrar</button>
        </form>
        <%-include('../footerGeneral');%>
    </div>

    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

    <!-- Option 2: Separate Popper and Bootstrap JS -->
